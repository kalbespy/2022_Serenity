<form method="post" action="{{ path('app_document_delete', {'id': document.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ document.id) }}">
	<button class="btn btn-danger m-2">Supprimer</button>
</form>
